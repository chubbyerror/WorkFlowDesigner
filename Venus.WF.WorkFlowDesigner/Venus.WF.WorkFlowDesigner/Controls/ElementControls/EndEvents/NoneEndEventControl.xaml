<UserControl x:Class="Venus.WF.WorkFlowDesigner.Controls.ElementControls.EndEvents.NoneEndEventControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Venus.WF.WorkFlowDesigner.Controls.ElementControls.EndEvents"
             mc:Ignorable="d" 
             >
    <Grid x:Name="RootGrid" Cursor="Hand" Opacity="0.5">
        <Ellipse x:Name="EllipseObject" Fill="White" Stroke="Black" StrokeThickness="5"></Ellipse>
        <TextBlock Text="None" Style="{StaticResource ElementControlTextBlockStyle}" ></TextBlock>
    </Grid>
</UserControl>
